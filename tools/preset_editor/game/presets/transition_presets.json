{
    "_comment": "Layered Transition Presets - Position-based animations",
    "_version": "2.0",
    "_usage": "show character at preset_slide_left_enter(), glow_blue",
    "_schema": {
        "start_position": "Use xalign/yalign (0.0-1.0) or x/y (pixels) or xoffset/yoffset",
        "end_position": "Same as start_position - system calculates delta automatically",
        "alpha": "start/end transparency (0.0-1.0)",
        "rotation": "start/end degrees",
        "scale": "start/end zoom (1.0 = normal)",
        "duration": "seconds",
        "easing": "linear, easein, easeout, ease"
    },
    "defaults": {
        "duration": 0.4,
        "easing": "easeout"
    },
    "presets": {
        "_comment_entrances": "========== ENTRANCE PRESETS ==========",
        "slide_left_enter": {
            "start_position": {
                "xoffset": -300
            },
            "end_position": {
                "xoffset": 0
            },
            "alpha": {
                "start": 0.0,
                "end": 1.0
            },
            "duration": 0.5,
            "easing": "easeout"
        },
        "slide_right_enter": {
            "start_position": {
                "xoffset": 0.0,
